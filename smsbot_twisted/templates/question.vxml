<?xml version="1.0" encoding="utf-8"?>
<vxml version="1.0" xmlns:smsbot="http://www.audacious-software.com/smsbot">
	<form id="tip">
		<field name="first" smsbot:message="first" smsbot:variable="question">
			<grammar>1|2</grammar>
			<prompt>Pick 1 or 2</prompt>
			<filled>
				<if cond="first=='1'">
					<block>Your selection was one.</block>
					<else/>
					<block>Your selection was greater than one.</block>
				</if>
			</filled>
		</field>
	</form>
</vxml>
